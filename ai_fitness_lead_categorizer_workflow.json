    {
      "parameters": {
        "jsCode": "return items.map((item, index) => {\n  // Each AI result item corresponds to the same lead in the same order\n  const category = item.json.choices?.[0]?.message?.content?.trim() || \"Uncategorized\";\n\n  // Clean up category string\n  const cleanCategory = category.replace(/^[\"'\\s]+|[\"'\\s]+$/g, \"\");\n\n  return {\n    json: {\n      ...item.json,\n      category: cleanCategory\n    }\n  };\n});\n"
